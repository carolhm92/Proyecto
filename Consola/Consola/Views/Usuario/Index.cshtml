@model IEnumerable<Consola.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Usuario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { item.IdUsuario, item.Usuario, item.Clave }) |
            @Html.ActionLink("Details", "Details", new { item.IdUsuario, item.Usuario, item.Clave }) |
            @Html.ActionLink("Delete", "Delete", new { item.IdUsuario, item.Usuario, item.Clave })
        </td>
    </tr>
}

</table>
